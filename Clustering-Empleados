s: inertia\n","from sklearn.cluster import KMeans \n"," \n","ks = range(2, 20) # crear valores del 2 al 20\n","inertias = []\n"," \n","for k in ks:\n","    # Crear  modelo\n","    model = KMeans(n_clusters=k)\n","    model.fit(data)\n","    inertias.append(model.inertia_)\n","    \n","# Graficar cantidad de clusters vs inertias\n","plt.plot(ks, inertias, '-o')\n","plt.xlabel('Numero de clusters, k')\n","plt.ylabel('inertia')\n","plt.xticks(ks)\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":279},"id":"sOIfdc6FnR02","executionInfo":{"status":"ok","timestamp":1666123018022,"user_tz":300,"elapsed":761,"user":{"displayName":"jean pierre agudelo","userId":"03166020082286009248"}},"outputId":"be15cefb-d5fe-4cf2-c2c2-33e604ad523f"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 432x288 with 1 Axes>"],"image/png":"\n"},"metadata":{"needs_background":"light"}}]},{"cell_type":"code","source":["#Método de la rodilla: silueta\n","from sklearn import metrics\n"," \n","ks = range(2, 20) # crear valores del 2 al 20\n","siluetas = []\n"," \n","for k in ks:\n","    # Crear  modelo\n","    model = KMeans(n_clusters=k)\n","    model.fit(data)\n","    sil=metrics.silhouette_score(data, model.labels_)\n","    siluetas.append(sil)\n","    \n","# Graficar cantidad de clusters vs inertias\n","plt.plot(ks, siluetas, '-o')\n","plt.xlabel('Numero de clusters, k')\n","plt.ylabel('silueta')\n","plt.xticks(ks)\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":279},"id":"_bTBrpFYnUi4","executionInfo":{"status":"ok","timestamp":1666123020195,"user_tz":300,"elapsed":2177,"user":{"displayName":"jean pierre agudelo","userId":"03166020082286009248"}},"outputId":"5fce7862-0571-472b-f77f-95269fd3d8e2"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 432x288 with 1 Axes>"],"image/png":"\n"},"metadata":{"needs_background":"light"}}]}